{
  "author": "Andreas Lind Petersen <andreas@one.com>",
  "name": "httperrors",
  "description": "Handy JavaScript Error classes representing the standard HTTP errors. Easily extensible and subclassable.",
  "keywords": [
    "http",
    "errors",
    "error",
    "request",
    "proxy",
    "client",
    "class",
    "errorclass"
  ],
  "version": "0.6.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/One-com/node-httperrors.git"
  },
  "main": "lib/httpErrors.js",
  "engines": {
    "node": ">= 0.2.0"
  },
  "dependencies": {
    "createerror": "0.5.0"
  },
  "devDependencies": {
    "browserify": "^5.3.0",
    "mocha": "1.12.1",
    "uglifyjs": "^2.3.6",
    "unexpected": "=0.0.6"
  },
  "scripts": {
    "dist": "mkdir -p dist && browserify ./lib/httpErrors.js -s httpErrors | uglifyjs -mc > ./dist/httpErrors.js",
    "prepublish": "npm run test && npm run dist",
    "test": "mocha --recursive"
  },
  "files": [
    "lib",
    "dist"
  ],
  "publishConfig": {
    "repository": "http://registry.npmjs.org/"
  }
}
